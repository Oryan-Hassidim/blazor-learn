{
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/_framework/*",
            "/css/*"
        ]
    },
    "auth": {
        "rolesSource": "/api/GetRoles",
        "identityProviders": {}
    },
    "routes": [
        {
            "route": "/.auth/login/twitter",
            "statusCode": 404
        },
        {
            "route": "/.auth/login/github",
            "statusCode": 404
        },
        {
            "route": "/admin/*",
            "allowedRoles": [ "admin" ]
        },
        {
            "route": "/api/products*",
            "methods": [ "PUT" ],
            "allowedRoles": [ "admin" ]
        }
    ]
}